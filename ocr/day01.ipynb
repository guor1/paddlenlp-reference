    "import cv2\n",
    " \n",
    "# 打开图像\n",
    "image = cv2.imread('example.jpg')\n",
    " \n",
    "# 显示图像\n",
    "cv2.imshow('Image', image)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()\n",
    " \n",
    "# 保存图像\n",
    "cv2.imwrite('output.jpg', image)"
   ]
  }
